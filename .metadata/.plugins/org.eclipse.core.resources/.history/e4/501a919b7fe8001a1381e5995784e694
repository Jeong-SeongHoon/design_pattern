package command2;

public class KubeInstaller extends Installer {
	public KubeInstaller(Env env) {
		super(env);
		// TODO Auto-generated constructor stub
	}

	@Override
	public void install() {
		// TODO Auto-generated method stub
		for (int i=0; i<this.env.getNodeList().length; i++) {
			this.env.getNodeList()[i].getOs().installKube();
		}
	}
}
