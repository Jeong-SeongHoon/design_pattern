package command1;

public final class Command {
	public static void centosInstallCmd() {
		System.out.println("centos install command exe");
	}
	public static void ubuntuInstallCmd() {
		System.out.println("ubuntu install command exe");
	}
}

class Solution { public int[] sortArrayByParity(int[] A) { int oi = 0; int ei = A.length - 1; while (oi < ei) { while (oi < A.length) { if (A[oi] % 2 != 0) break; oi++; } if (oi == A.length) break; while (ei >= 0) { if (A[ei] % 2 == 0) break; ei--; } if (ei == -1) break; if (oi < ei) { int tmp = A[oi]; A[oi] = A[ei]; A[ei] = tmp; } oi++; ei--; } return A; } }
