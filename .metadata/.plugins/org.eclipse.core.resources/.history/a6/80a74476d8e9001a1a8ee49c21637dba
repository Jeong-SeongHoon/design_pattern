package command2;

public class Env {
	private Node[] nodeList = new Node[2];

	public Env() {
		super();
		this.nodeList[0] = new Node(new Centos());
		this.nodeList[1] = new Node(new Ubuntu());
	}

	public Node[] getNodeList() {
		return nodeList;
	}

	public void setNodeList(Node[] nodeList) {
		this.nodeList = nodeList;
	}
	
	
}
